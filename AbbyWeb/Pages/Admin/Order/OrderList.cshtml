@page
@using System.Diagnostics
@model AbbyWeb.Pages.Admin.Order.OrderListModel
@{
}
<table class="table table-bordered table-striped" style="width:100%">
  <thead>
    <tr class="table-row">
      <th>ORDER ID</th>
      <th>NAME</th>
      <th>EMAIL</th>
      <th>ORDER TOTAL</th>
      <th>PICKUP TIME</th>
      <th></th>
    </tr>
  </thead>
  <tbody>
    @if (Model.resData != null)
    {
      @foreach (var item in @Model.resData)
      {
        <tr>
          <td>@item.Id</td>
          <td>@item.ApplicationUser.FirstName @item.ApplicationUser.LastName</td>
          <td>@item.ApplicationUser.Email</td>
          <td>@item.OrderTotal</td>
          <td>@item.PickUpTime</td>
          <td>
            <div class="w-75 btn-group" role="group">
              <a asp-page="OrderDetails" asp-route-id="@item.Id" class="btn btn-primary mx-2">
                <i class="bi bi-pencil-square"></i>
              </a>
            </div>
          </td>
        </tr>
      }
    }
  </tbody>
</table>
